    String Jenis_Kelamin = input.next();
    
    System.out.print("Masukan Tinggi Badan Anda = ");
    int tingi_badan = input.nextInt();
    
    if (Jenis_Kelamin == "Laki-Laki") {
    }else if (tingi_badan > 170){
        System.out.print("Selemat Anda Lulus");
    }else{
        System.out.print("Gagal");
        
        if(Jenis_Kelamin == "Perempuan"){
        }else if (tingi_badan > 162){
            System.out.print("Selamat Anda Lulus");
        }else{
            System.out.print("Anda Gagal");
        }
    }
    }
 }
